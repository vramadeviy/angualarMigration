@import '../../../theme/styles/table';

.year-end-supplemental-report-wrapper{
  
    .filter-container {
      .flex-layout {
        display: flex;
        .form-field {
          margin-left: 7px;
        }
      }
    }
    .report-body-wrapper {
      margin-top: 20px;
      height: 560px;
    }
    .report-history-detail {
      .pane-hScroll {
        width: 77%;
      }
      .preference-table-header {
        th {
          padding: 5px;
          height: auto;
        }
      }
      .pane-vScroll {
        overflow: auto;
        height: 200px;
        &::-webkit-scrollbar {
          width: 9px;
          height: 22px;
        }
        tr{
          height: auto !important;
          td {
            padding: 5px !important;
          }
        } 
      }
    }
  }
  